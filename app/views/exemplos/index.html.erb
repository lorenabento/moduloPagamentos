<h1>Listing exemplos</h1>

<table>
  <tr>
    <th>Nome</th>
    <th>Idade</th>
    <th>Telefone</th>
    <th>Datacadastro</th>
    <th>Ativo</th>
    <th>Logradouro</th>
    <th>Cidade</th>
    <th>Uf</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @exemplos.each do |exemplo| %>
  <tr>
    <td><%= exemplo.nome %></td>
    <td><%= exemplo.idade %></td>
    <td><%= exemplo.telefone %></td>
    <td><%= exemplo.datacadastro %></td>
    <td><%= exemplo.ativo %></td>
    <td><%= exemplo.logradouro %></td>
    <td><%= exemplo.cidade %></td>
    <td><%= exemplo.uf %></td>
    <td><%= link_to 'Show', exemplo %></td>
    <td><%= link_to 'Edit', edit_exemplo_path(exemplo) %></td>
    <td><%= link_to 'Destroy', exemplo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Exemplo', new_exemplo_path %>
