class Issue < ActiveRecord::Base
attr_accessible :id, :release_id, :author_id, :validator_id, :client_id, :feature_id, :type_id, :status_id, :os_version_id, :browser_version_id, :mobile_model_id, :test_case_run_id, :number, :title, :description, :action_performed, :expected_results, :additional_info, :attachments_count
end
